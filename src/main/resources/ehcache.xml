<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true"
	monitoring="autodetect" dynamicConfig="true">


	<cache name="commits" maxBytesLocalHeap="20m" eternal="false" timeToLiveSeconds="3600"/>
	<cache name="users" maxBytesLocalHeap="10m" eternal="false" timeToLiveSeconds="3600"/>
	<cache name="repositories" maxBytesLocalHeap="10m" eternal="false" timeToLiveSeconds="3600"/>
	<cache name="autosuggests" maxBytesLocalHeap="10m" eternal="false" timeToLiveSeconds="86400"/>


</ehcache>
